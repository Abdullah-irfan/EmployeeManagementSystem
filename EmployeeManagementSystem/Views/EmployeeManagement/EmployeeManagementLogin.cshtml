@model EmployeeManagement.Core.Modal.LoginDetails



<h1>Student Mark Details</h1>
@using (Html.BeginForm("EmployeeManagementLogin", "EmployeeManagement", FormMethod.Post, new { role = "form", id = "formDetails" }))
{ 
<div class="container">
    <div class="form-group">
        @Html.LabelFor(x => x.UserName, new { @class = "form-label" })<span>*</span>

        @Html.TextBoxFor(x => x.UserName, new { @class = "form-control", @placeholder = "Enter Your Email" })
    </div>

    <div class="form-group">
        @Html.LabelFor(x => x.Password, new { @class = "form-label" })<span>*</span>

        @Html.TextBoxFor(x => x.Password, new { @class = "form-control", @placeholder = "Enter Your Email" })
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-primary" onclick="SubmitDetails()">Submit</button>
    </div>

</div>
}